    "format": "prettier . --write",
    "generate-types": "bunx supabase gen types typescript --project-id xzbxgpjvspkmtsrjjfgj --schema public > lib/supabase/database.types.ts"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
