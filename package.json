    "trailingComma": "none",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ],
    "organizeImportsSkipDestructiveCodeActions": true,
    "tailwindStylesheet": "./app/globals.css",
    "tailwindFunctions": [
      "twMerge",
      "clsx",
      "cn"
